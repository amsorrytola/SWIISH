{
  "name": "swiish-backend",
  "version": "1.0.0",
  "description": "SWIISH DeFi Telegram Mini App Backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build-frontend": "cd ../Frontend && npm run build",
    "build-and-start": "npm run build-frontend && npm start",
    "install-all": "npm install && cd ../Frontend && npm install",
    "dev-frontend": "cd ../Frontend && npm run dev",
    "install-deps": "npm install",
    "setup": "npm install && npm start",
    "clean-install": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
